<widget id="com.kreaserv.kreaserv" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kreaserv</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <platform name="android">
        <!-- 15 -->
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>

    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>

    <preference name="android-build-tool" value="gradle" />
    <preference name="DisallowOverscroll" value="true" />



    <icon src="www/img/icon.jpg" width="72" height="72" />
    <icon src="www/img/icon.jpg" width="48" height="48" />
    <icon src="www/img/icon.jpg" width="36" height="36" />


    
<!--     <preference name="fullscreen" value="false" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize"/> -->


    <!-- fixed version -->



    <plugin spec="https://github.com/leecrossley/cordova-plugin-directions.git#35b90f2" />
