    <plugin spec="https://github.com/apache/cordova-plugin-network-information.git#09b46b2" />
    <plugin spec="https://github.com/apache/cordova-plugin-camera.git#d124e03" />
    <plugin spec="https://github.com/apache/cordova-plugin-file-transfer.git#a4031db" />

    <!-- v0.12.0 -->

    <plugin spec="https://github.com/Wizcorp/phonegap-facebook-plugin.git#v0.12.0">
        <param name="APP_ID" value="649298491886916" />
        <param name="APP_NAME" value="mumbai parties" />
    </plugin>

